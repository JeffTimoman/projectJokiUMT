<?xml version='1.0' encoding='utf-8'?>
<widget id="lgarin211.ptk.tujuhd" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Tefaktika</name>
    <description>telaah filosofis penelitian tindakan kelas</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        telaah filosofis penelitian tindakan kelas
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />

    <icon src="www/res/icon.jpg" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="none" />
    <preference name="SplashScreenDelay" value="1000" />
    <preference name="AndroidWindowSplashScreenBackground" value="#121212" />
    <preference name="AndroidWindowSplashScreenAnimatedIcon" value="www/res/icon.jpg" />
</widget>
